{
    "name": "theihasan/dual-agent",
    "description": "Dual agent monitoring for Laravel Nightwatch with database storage",
    "keywords": ["laravel", "nightwatch", "monitoring", "metrics", "database"],
    "license": "MIT",
    "authors": [
        {
            "name": "Ihasan",
            "email": "theihasan@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/framework": "^10.0|^11.0|^12.0",
        "laravel/nightwatch": "^1.0",
        "illuminate/support": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^10.0",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Ihasan\\DualAgent\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ihasan\\DualAgent\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ihasan\\DualAgent\\DualAgentServiceProvider"
            ],
            "aliases": {
                "DualAgent": "Ihasan\\DualAgent\\Facades\\DualAgent"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
